<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.progressuas.Main_menu_Controller">
   <children>
      <ImageView fitHeight="450.0" fitWidth="650.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/mainmenu.png" />
         </image>
      </ImageView>
      <Label layoutX="202.0" layoutY="20.0" text="Manajemen Stock Barang" textFill="WHITE">
         <font>
            <Font name="Serif Bold" size="22.0" />
         </font>
      </Label>
      <Label layoutX="278.0" layoutY="45.0" text="Alat Tulis" textFill="WHITE">
         <font>
            <Font name="Serif Bold" size="22.0" />
         </font>
      </Label>
      <Button fx:id="stockbarang" layoutX="384.0" layoutY="133.0" mnemonicParsing="false" onAction="#StockBarang" prefHeight="33.0" prefWidth="213.0" style="-fx-background-color: #D9D9D9;" text="Stock Barang" />
      <Button fx:id="bantuan" layoutX="384.0" layoutY="194.0" mnemonicParsing="false" onAction="#Bantuan" prefHeight="33.0" prefWidth="213.0" style="-fx-background-color: #D9D9D9;" text="Bantuan" />
      <ImageView fitHeight="33.0" fitWidth="37.0" layoutX="411.0" layoutY="134.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/stock.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="25.0" fitWidth="37.0" layoutX="415.0" layoutY="199.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/Bantuan.png" />
         </image>
      </ImageView>
      <Button fx:id="keluar" layoutX="384.0" layoutY="254.0" mnemonicParsing="false" onAction="#Exit" prefHeight="33.0" prefWidth="213.0" style="-fx-background-color: #D9D9D9;" text="Exit" />
      <ImageView fitHeight="25.0" fitWidth="30.0" layoutX="415.0" layoutY="258.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/exit.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
